extends ../layouts/default
block content
    section
      #inner-banner
      #inner-content-box#content-box
        .container
          #offerings
            .col-md-9.text-right
              ul.list-inline
                li
                  a(href="#",onclick="printFunction()" target="_blank")
                    i.fa.fa-lg.fa-print
                li
                  a#pdf
                    i.fa.fa-lg.fa-file-pdf-o
                li
                  a(href="/contactform")
                    i.fa.fa-lg.fa-envelope
            .col-md-9#content-data
              h2 #{data.lifestyleindustrys[0].heading}
              .row
                .col-md-4
                  if data.lifestyleindustrys.length
                        img(id="image",src=data.lifestyleindustrys[0]._.image.limit(640,640))
                .col-md-8
            include ../views/footermenunavigation